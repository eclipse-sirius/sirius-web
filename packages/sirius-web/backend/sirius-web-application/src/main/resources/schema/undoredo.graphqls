extend type Mutation {
    undo(input : UndoInput!) : UndoPayload!
    redo(input : RedoInput!) : UndoPayload!
}

input RedoInput {
    id: ID!
    editingContextId: ID!
    inputId: ID!
}

input UndoInput {
    id: ID!
    editingContextId: ID!
    inputId: ID!
}

union UndoPayload = ErrorPayload | SuccessPayload

union RedoPayload = ErrorPayload | SuccessPayload